<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8" />
	<title>{% block title %}Revestimientos J&G - Barquisimeto{% endblock %}</title>
	<link rel="stylesheet" type="text/css" href="{{ asset('bundles/jygrevestimientos/css/bootstrap.css') }}">
	{% block stylesheets %}{% endblock %}
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="{{ path('JYGRevestimientosBundle_inicio') }}">Revestimientos J&G</a>
			</div>
			<div id="navbar" class="navbar-collapse collapse">
				<ul class="nav navbar-nav navbar-right">
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'index' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_inicio') }}">Inicio</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'productos' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_productos') }}">Productos</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'galeria' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_galeria') }}">Galería</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'vision' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_vision') }}">Visión</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'mision' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_mision') }}">Misión</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'contacto' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_contacto') }}">Contacto</a></li>
					<li class="{{ app.request.attributes.get('_route') | replace({'jyg_revestimientos_page_' : ''}) == 'ayuda' ? 'active' }}"><a href="{{ path('JYGRevestimientosBundle_ayuda') }}">Ayuda</a></li>
				</ul>
				<!--<ul class="nav navbar-nav pull-right">
					<li class="dropdown">
						<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
						<ul class="dropdown-menu dropdown-menu-right">
							<li><a href="#">Action</a></li>
							<li><a href="#">Another action</a></li>
							<li><a href="#">Something else here</a></li>
							<li role="separator" class="divider"></li>
							<li class="dropdown-header">Nav header</li>
							<li><a href="#">Separated link</a></li>
							<li><a href="#">One more separated link</a></li>
						</ul>
					</li>
				</ul> -->
			</div><!--/.nav-collapse -->
		</div>
	</nav>
	<div class="container navSpace">
		{% block body %}{% endblock %}
	</div>
	<div class="container">
		<!-- FOOTER -->
		<footer class="foooter">
			<p class="pull-right">{{ "now" | date("F jS", "America/Caracas") }} &middot; <a href="#">Subir</a></p>
			<p>&copy; {{ "now" | date("Y") }} Revestimientos J&G</a></p>
		</footer>

	</div><!-- /.container -->

	<script src="{{ asset('bundles/jygrevestimientos/js/jquery.min.js') }}"></script>
	<script src="{{ asset('bundles/jygrevestimientos/js/bootstrap.min.js') }}"></script>
	{% block javascripts %}{% endblock %}
</body>
</html>
