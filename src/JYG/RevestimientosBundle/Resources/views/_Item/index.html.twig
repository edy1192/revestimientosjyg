                <td>{{ entity.descripcionmaterial }}</td>
                <td>{{ entity.codigomaterial }}</td>
                <td>{{ entity.venta }}</td>
                <!--<td>
                <ul>
                    <li>
                        <a href="{#{ path('item_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('item_edit', { 'id': entity.id }) }#}">edit</a>
                    </li>
                </ul>
                </td>-->
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
